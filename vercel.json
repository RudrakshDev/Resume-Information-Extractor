{
    "version": 2,
    "builds": 
    [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": 
    [
        {
            "src": "/_next/static/(.*)",
            "dest": "/_next/static/$1"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": 
    {
        "NODE_ENV": "production",
        "TURBOPACK": "0",
        "NEXT_DISABLE_TURBOPACK": "1"
    }
}